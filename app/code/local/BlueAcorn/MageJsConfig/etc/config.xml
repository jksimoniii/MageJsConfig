<config>
    <modules>
        <BlueAcorn_MageJsConfig>
            <version>1.0.0</version>
        </BlueAcorn_MageJsConfig>
    </modules>

    <global>
        <helpers>
            <blueacorn_magejsconfig>
                <class>BlueAcorn_MageJsConfig_Helper</class>
            </blueacorn_magejsconfig>
        </helpers>
        <models>
            <blueacorn_magejsconfig>
                <class>BlueAcorn_MageJsConfig_Model</class>
            </blueacorn_magejsconfig>
        </models>
    </global>

    <frontend>
        <layout>
            <updates>
                <blueacorn_magejsconfig>
                    <file>blueacorn/magejsconfig.xml</file>
                </blueacorn_magejsconfig>
            </updates>
        </layout>
    </frontend>
</config>